<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0"
	xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="view.LoadGameController">
	<children>
		<AnchorPane prefHeight="720.0" prefWidth="1080.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<VBox alignment="CENTER" maxHeight="600.0" maxWidth="1080.0"
					prefHeight="600.0" prefWidth="1080.0" spacing="80.0">
					<children>
						<Button fx:id="saveGame1" mnemonicParsing="false" onAction="#handleSaveGame"
							prefHeight="82.0" prefWidth="593.0" text="Save_Game_1" />
						<Button fx:id="saveGame2" mnemonicParsing="false" onAction="#handleSaveGame"
							prefHeight="82.0" prefWidth="593.0" text="Save_Game_2" />
						<Button fx:id="saveGame3" mnemonicParsing="false" onAction="#handleSaveGame"
							prefHeight="82.0" prefWidth="593.0" text="Save_Game_3" />
					</children>
				</VBox>
				<HBox alignment="CENTER" layoutY="586.0" maxHeight="120.0"
					maxWidth="1080.0" prefHeight="120.0" prefWidth="1080.0" spacing="205.0">
					<children>
						<Button fx:id="smazat" mnemonicParsing="false" onAction="#handleSmazat"
							prefHeight="68.0" prefWidth="88.0" text="Smazat" />
						<Button fx:id="nacist" mnemonicParsing="false" onAction="#handleNacist"
							prefHeight="68.0" prefWidth="88.0" text="Načíst" />
						<Button fx:id="ulozit" mnemonicParsing="false" onAction="#handleUlozit"
							prefHeight="68.0" prefWidth="88.0" text="Uložit" />
						<Button fx:id="zpet" mnemonicParsing="false" onAction="#handleZpet"
							prefHeight="68.0" prefWidth="88.0" text="Zpět" />
					</children>
				</HBox>
			</children>
		</AnchorPane>
	</children>
</AnchorPane>
