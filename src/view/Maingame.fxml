<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainGameControler">
   <children>
      <BorderPane prefHeight="800.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <bottom>
            <HBox prefHeight="59.0" prefWidth="800.0" BorderPane.alignment="CENTER">
               <children>
                  <MenuButton alignment="CENTER" mnemonicParsing="false" prefHeight="63.0" prefWidth="177.0" text="Menu">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Load Game" />
                      <MenuItem mnemonicParsing="false" text="Options" />
                        <MenuItem mnemonicParsing="false" text="Main Menu" />
                    </items>
                  </MenuButton>
                  <Button fx:id="Inventory" mnemonicParsing="false" onAction="#OpenInventory" prefHeight="63.0" prefWidth="177.0" text="Inventory" />
               </children>
            </HBox>
         </bottom>
         <center>
            <GridPane prefHeight="554.0" prefWidth="744.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
